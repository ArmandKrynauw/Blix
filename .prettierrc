{
	"printWidth": 100,
	"singleQuote": false,
	"trailingComma": "es5",
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"pluginSearchDirs": false,
	"svelteSortOrder": "scripts-markup-styles",
	"svelteStrictMode": true,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
